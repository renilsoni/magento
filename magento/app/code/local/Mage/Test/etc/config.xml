<?xml version="1.0"?>
<config>
	<modules>
		<Mage_Test>
			<version>1.0.0</version>
		</Mage_Test>
	</modules>
	<frontend>
		<routers>
			<test>
				<use>standard</use>
				<args>
					<module>Mage_Test</module>
					<frontName>test</frontName>
				</args>
			</test>
		</routers>
		<layout>
			<updates>
				<test>
					<file>test.xml</file>
				</test>
			</updates>
		</layout>
	</frontend>
	<global>
		<blocks>
			<test>
				<class>Mage_Test_Block</class>
			</test>
		</blocks>
	</global>
</config>